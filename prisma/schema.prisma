generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mongodb"
    url      = env("DATABASE_URL")
}

model WebsiteSummary {
    id             String   @id @default(auto()) @map("_id") @db.ObjectId
    url            String
    about          String
    terms          String
    policies       String
    data_collected String
    performance    Int?
    accessibility  Int?
    bestPractices  Int?
    seo            Int?
    userId         String?
    createdAt      DateTime @default(now())
}
